<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import org.controlsfx.control.Rating?>

<HBox fx:id="tour1" alignment="CENTER" prefHeight="170.0" prefWidth="735.0" spacing="10.0" styleClass="hbox-tour" stylesheets="@../../../../../css/screen7_Tours.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sk.stu.fiit.Main.TourOfferController">
   <children>
      <ImageView fx:id="photo" fitHeight="150.0" fitWidth="157.0" pickOnBounds="true" smooth="false" styleClass="photo-tour">
         <HBox.margin>
            <Insets left="15.0" top="20.0" />
         </HBox.margin>
      </ImageView>
      <Pane prefHeight="150.0" prefWidth="305.0">
         <children>
            <Label layoutX="14.0" layoutY="45.0" styleClass="lbl-tour" text="%tourOffer.startPlaceLabel.text">
               <effect>
                  <DropShadow color="#0000001a" height="30.0" offsetY="2.0" radius="14.5" spread="0.75" width="30.0" />
               </effect>
            </Label>
            <Label layoutX="15.0" layoutY="17.0" styleClass="lbl-tour" text="%tourOffer.nameLabel.text">
               <effect>
                  <DropShadow color="#0000001a" height="30.0" offsetY="2.0" radius="14.5" spread="0.75" width="30.0" />
               </effect></Label>
            <Label layoutX="14.0" layoutY="73.0" styleClass="lbl-tour" text="%tourOffer.destinationLabel.text">
               <effect>
                  <DropShadow color="#0000001a" height="30.0" offsetY="2.0" radius="14.5" spread="0.75" width="30.0" />
               </effect></Label>
            <Label layoutX="14.0" layoutY="101.0" styleClass="lbl-tour" text="%tourOffer.ratingLabel.text">
               <effect>
                  <DropShadow color="#0000001a" height="30.0" offsetY="2.0" radius="14.5" spread="0.75" width="30.0" />
               </effect></Label>
            <Label layoutX="14.0" layoutY="129.0" styleClass="lbl-tour" text="%tourOffer.priceLabel.text">
               <effect>
                  <DropShadow color="#0000001a" height="30.0" offsetY="2.0" radius="14.5" spread="0.75" width="30.0" />
               </effect></Label>
            <Label fx:id="lblStartPlace" layoutX="104.0" layoutY="45.0" styleClass="lbl-informations" text="Label">
               <effect>
                  <DropShadow color="#0000001a" height="30.0" offsetY="2.0" radius="14.5" spread="0.75" width="30.0" />
               </effect>
            </Label>
            <Label fx:id="lblName" layoutX="68.0" layoutY="17.0" styleClass="lbl-informations" text="Label">
               <effect>
                  <DropShadow color="#0000001a" height="30.0" offsetY="2.0" radius="14.5" spread="0.75" width="30.0" />
               </effect></Label>
            <Label fx:id="lblDestination" layoutX="103.0" layoutY="73.0" styleClass="lbl-informations" text="Label">
               <effect>
                  <DropShadow color="#0000001a" height="30.0" offsetY="2.0" radius="14.5" spread="0.75" width="30.0" />
               </effect></Label>
            <Label fx:id="lblPrice" layoutX="60.0" layoutY="129.0" styleClass="lbl-informations" text="Label">
               <effect>
                  <DropShadow color="#0000001a" height="30.0" offsetY="2.0" radius="14.5" spread="0.75" width="30.0" />
               </effect></Label>
            <Rating fx:id="starsRating" layoutX="77.0" layoutY="96.0" partialRating="true" />
         </children>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Pane>
      <Button fx:id="btnInterested" mnemonicParsing="false" onMouseReleased="#handleBtnInterested" styleClass="btn-interested" text="%tourOffer.interestedButton.text">
         <HBox.margin>
            <Insets left="25.0" top="105.0" />
         </HBox.margin>
         <effect>
            <DropShadow color="#00000026" height="30.0" offsetY="2.0" radius="14.5" spread="0.3" width="30.0" />
         </effect>
      </Button>
   </children>
   <effect>
      <DropShadow color="#00000026" height="30.0" offsetY="2.0" radius="14.5" spread="0.05" width="30.0" />
   </effect>
</HBox>
